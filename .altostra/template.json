{
	"name": "Static Website",
	"description": "A static website with CDN distribution",
	"includes": [
		"/public/**"
	],
	"excludes": [],
	"blueprints": [
		{
			"id": "StaticWebSite",
			"name": "Static Website",
			"description": "Static Website Template",
			"version": "2019-10-24",
			"connections": {
				"DomainName01": {
					"SiteCdn01": {
						"from": "DomainName01",
						"to": "SiteCdn01",
						"type": "connection.domain-name"
					}
				},
				"SiteCdn01": {
					"SiteFiles01": {
						"bucketRoot": "",
						"from": "SiteCdn01",
						"route": "",
						"to": "SiteFiles01",
						"type": "connection.dependency.cdn-storage"
					}
				}
			},
			"metadata": {
				"designer": {
					"resourcePositions": {
						"DomainName01": {
							"x": 172,
							"y": 225,
							"z": 0
						},
						"SiteCdn01": {
							"x": 409,
							"y": 282,
							"z": 0
						},
						"SiteFiles01": {
							"x": 648,
							"y": 282,
							"z": 0
						}
					}
				}
			},
			"parameters": {
				"DOMAIN_CERT": {
					"paths": [
						"resources/DomainName01/certificate/uri"
					]
				},
				"DOMAIN_NAME": {
					"paths": [
						"resources/DomainName01/dnsName"
					]
				}
			},
			"resources": {
				"DomainName01": {
					"certificate": {
						"type": "cert.stored",
						"uri": ""
					},
					"dnsName": "www.example.com",
					"id": "DomainName01",
					"name": "domain-name",
					"type": "resource.dns.name"
				},
				"SiteCdn01": {
					"defaultRoot": "index.html",
					"id": "SiteCdn01",
					"isSPA": false,
					"name": "site-cdn",
					"tls": {
						"httpRedirection": true,
						"useTLS": true
					},
					"type": "resource.cdn"
				},
				"SiteFiles01": {
					"accessLevel": "public-read",
					"filesSources": [
						{
							"path": "public",
							"type": "files-source.project-dir"
						}
					],
					"id": "SiteFiles01",
					"name": "site-files",
					"type": "resource.file-store"
				}
			}
		}
	]
}
